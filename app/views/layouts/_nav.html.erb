<div class="row">
  <div class="col log_in_nav logged_in">
    <% if current_user %>
      Logged in as <br /><%= current_user.email %> <br />
      <%= link_to "Log out", logout_path %>
    <% else %>
      <li></li><%= link_to "Sign up", signup_path %> or
      <%= link_to "log in", login_path %>
    <% end %>
  </div>
</div>
